<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwlly.dao.SpeakerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lwlly.pojo.Speaker">
        <id column="id" property="id" />
        <result column="speaker_name" property="speakerName" />
        <result column="speaker_desc" property="speakerDesc" />
        <result column="speaker_job" property="speakerJob" />
        <result column="head_img_url" property="headImgUrl" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, speaker_name, speaker_desc, speaker_job, head_img_url
    </sql>

</mapper>
